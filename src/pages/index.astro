import { SITE_TITLE, SITE_DESCRIPTION } from "../consts";
import Nav from "../components/Nav.astro";
import {Content as HomeContent} from './home.md';
---
<!doctype html>
<html lang="en">
  <head>
    <BaseHead title="home" description={SITE_DESCRIPTION} />
  </head>
  <body>
    <Header>
      <Nav />
    </Header>
    <main>
        <HomeContent />
    </main>
    <Footer />
  </body>
</html>
